{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "MLflowArtifactsBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::mlflow-artifacts-armando"
    },
    {
      "Sid": "MLflowArtifactsObjects",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::mlflow-artifacts-armando/*"
    }
  ]
}
